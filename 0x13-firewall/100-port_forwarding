#!/bin/bash

# Define the web server IP address
web_server_ip="54.175.59.46"

# Configure the port redirection rule
iptables -t nat -A PREROUTING -p tcp -d $web_server_ip --dport 8080 -j REDIRECT --to-port 80

# Save the rules
iptables-save > /etc/iptables/rules.v4

# Restart the firewall to apply the changes
